						"clientCredential": {
							"clientSecretSettingName": "3RRape0SdzY+dAs/FnZJ/29SlXojDFuHmPzszV9nE1/oZ8mZPbFl9JfGFuZfQCp3msjwB+gkQFNVWZTPpiXxng=="
						},
						"openIdConnectConfiguration": {
